		<gl-cor:summaryReportingElement contextRef="H1L4">usfr-pte_UnrestrictedCash</gl-cor:summaryReportingElement>
	</gl-cor:xbrlInfo.Detail>
	<gl-cor:entryDetail.Detail>
		<gl-cor:lineNumber contextRef="H1L5">5</gl-cor:lineNumber>
		<gl-cor:specified.account contextRef="H1L5"/>
		<gl-cor:amount contextRef="H1L5" decimals="2" unitRef="usd">168416.25</gl-cor:amount>
		<gl-cor:debitCreditCode contextRef="H1L5">D</gl-cor:debitCreditCode>
		<gl-cor:postingDate contextRef="H1L5">2005-06-01</gl-cor:postingDate>
