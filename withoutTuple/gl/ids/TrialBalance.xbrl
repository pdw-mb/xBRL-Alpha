	</gl-cor:xbrlInfo.Detail>
	<gl-cor:entryDetail.Detail>
		<gl-cor:lineNumber contextRef="H1L3">3</gl-cor:lineNumber>
		<gl-cor:specified.account contextRef="H1L3"/>
		<gl-cor:amount contextRef="H1L3" decimals="2" unitRef="usd">104468.02</gl-cor:amount>
		<gl-cor:debitCreditCode contextRef="H1L3">D</gl-cor:debitCreditCode>
		<gl-cor:postingDate contextRef="H1L3">2005-06-01</gl-cor:postingDate>
		<gl-cor:specified.xbrlInfo contextRef="H1L3"/>
	</gl-cor:entryDetail.Detail>
	<gl-cor:account.Detail>
		<gl-cor:accountMainID contextRef="H1L3">1003</gl-cor:accountMainID>
		<gl-cor:accountMainDescription contextRef="H1L3">Eagle Bank</gl-cor:accountMainDescription>
		<gl-cor:accountPurposeCode contextRef="H1L3">usgaap</gl-cor:accountPurposeCode>
		<gl-cor:accountType contextRef="H1L3">account</gl-cor:accountType>
	</gl-cor:account.Detail>
	<gl-cor:xbrlInfo.Detail>
		<gl-cor:xbrlInclude contextRef="H1L3">beginning_balance</gl-cor:xbrlInclude>
		<gl-cor:summaryReportingElement contextRef="H1L3">usfr-pte_UnrestrictedCash</gl-cor:summaryReportingElement>
	</gl-cor:xbrlInfo.Detail>
	<gl-cor:entryDetail.Detail>
		<gl-cor:lineNumber contextRef="H1L4">4</gl-cor:lineNumber>
		<gl-cor:specified.account contextRef="H1L4"/>
		<gl-cor:amount contextRef="H1L4" decimals="2" unitRef="usd">77033.02</gl-cor:amount>
		<gl-cor:debitCreditCode contextRef="H1L4">D</gl-cor:debitCreditCode>
		<gl-cor:postingDate contextRef="H1L4">2005-06-30</gl-cor:postingDate>
