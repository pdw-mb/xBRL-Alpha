		<gl-cor:accountType contextRef="now">account</gl-cor:accountType>
	</gl-cor:account.Detail>
	<gl-cor:xbrlInfo.Detail>
		<gl-cor:xbrlInclude contextRef="now">period_change</gl-cor:xbrlInclude>
		<gl-cor:summaryReportingElement contextRef="now">usfr-pte_UnrestrictedCash</gl-cor:summaryReportingElement>
