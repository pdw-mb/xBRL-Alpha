	<xbrli:context id="H2">
		<xbrli:entity>
			<xbrli:identifier scheme="http://www.xbrl.org/xbrlgl/sample">SAMPLE</xbrli:identifier>
		</xbrli:entity>
		<!-- The XBRL GL WG recommends using the file creation data as the period. -->
		<xbrli:period>
			<xbrli:instant>2004-10-03</xbrli:instant>
		</xbrli:period>
		<xbrli:scenario>
			<xbrldi:typedMember dimension="gl-cor:dEntryNumber">
				<gl-cor:EntryNumber>H2</gl-cor:EntryNumber>
			</xbrldi:typedMember>
		</xbrli:scenario>
	</xbrli:context>
	<xbrli:context id="H2L1">
		<xbrli:entity>
			<xbrli:identifier scheme="http://www.xbrl.org/xbrlgl/sample">SAMPLE</xbrli:identifier>
		</xbrli:entity>
		<!-- The XBRL GL WG recommends using the file creation data as the period. -->
		<xbrli:period>
