<!-- The "root" of every XBRL file ("instance document") is xbrl. -->
<xbrli:xbrl xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xbrli="http://www.xbrl.org/2003/instance" xmlns:xbrll="http://www.xbrl.org/2003/linkbase" xmlns:xbrldi="http://xbrl.org/2006/xbrldi" xmlns:gl-cor="http://www.xbrl.org/int/gl/cor/2020-12-31" xmlns:gl-muc="http://www.xbrl.org/int/gl/muc/2020-12-31" xmlns:gl-bus="http://www.xbrl.org/int/gl/bus/2020-12-31" xmlns:gl-plt="http://www.xbrl.org/int/gl/plt/2020-12-31" xmlns:iso4217="http://www.xbrl.org/2003/iso4217" xmlns:iso639="http://www.xbrl.org/2005/iso639" xsi:schemaLocation="http://www.xbrl.org/int/gl/plt/2020-12-31 ../plt/case-c-b-m-u-t/gl-plt-all-2020-12-31.xsd">
	<xbrll:schemaRef xlink:href="../plt/case-c-b-m-u-t/gl-plt-all-2020-12-31.xsd" xlink:type="simple" xlink:arcrole="http://www.w3.org/1999/xlink/properties/linkbase"/>
	<!-- Contexts, mandatory according to the XBRL 2.1, Specification, are not meant to describe the information in XBRL GL and appear only by convention. All facts are instant and developers are encouraged to duplicate/provide the date the XBRL GL information is created as the period's date. -->
	<xbrli:context id="now">
